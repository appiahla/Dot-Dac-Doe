<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/icontpage"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".iconPage">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="66dp"
        android:fontFamily="@font/gloria_hallelujah"
        android:text="Player 1"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <TextView
        android:id="@+id/how_to_play2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="103dp"
        android:layout_marginEnd="66dp"
        android:fontFamily="@font/gloria_hallelujah"
        android:text="Player 2"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="149dp"
        android:layout_height="145dp"
        android:layout_marginStart="41dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/user_circle" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="149dp"
        android:layout_height="145dp"
        android:layout_marginEnd="41dp"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/how_to_play2"
        app:srcCompat="@drawable/user_circle" />
<!--        app:srcCompat="@drawable/screen_shot_2022_02_27_at_5_30_45_pm" />-->

        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/user_circle" />


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="62dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="333dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/screen_shot_2022_02_27_at_5_35_57_pm" />

    <ImageView
        android:id="@+id/tree"
        android:layout_width="107dp"
        android:layout_height="100dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toStartOf="@+id/egg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:srcCompat="@drawable/row_1_column_1" />

    <ImageView
        android:id="@+id/fries"
        android:layout_width="107dp"
        android:layout_height="100dp"
        android:layout_marginStart="45dp"
        app:layout_constraintBottom_toTopOf="@+id/planet"
        app:layout_constraintEnd_toStartOf="@+id/wave"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tree"
        app:srcCompat="@drawable/row_2_column_1" />

    <ImageView
        android:id="@+id/planet"
        android:layout_width="107dp"
        android:layout_height="100dp"
        android:layout_marginStart="45dp"
        app:layout_constraintBottom_toTopOf="@+id/flower"
        app:layout_constraintEnd_toStartOf="@+id/rain"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fries"
        app:srcCompat="@drawable/row_3_column_1" />

    <ImageView
        android:id="@+id/flower"
        android:layout_width="107dp"
        android:layout_height="100dp"
        android:layout_marginStart="45dp"
        app:layout_constraintBottom_toTopOf="@+id/lightning"
        app:layout_constraintEnd_toStartOf="@+id/goggles"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/planet"
        app:srcCompat="@drawable/row_4_column_1" />

    <ImageView
        android:id="@+id/goggles"
        android:layout_width="107dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toTopOf="@+id/smile"
        app:layout_constraintEnd_toStartOf="@+id/paint"
        app:layout_constraintStart_toEndOf="@+id/flower"
        app:layout_constraintTop_toBottomOf="@+id/rain"
        app:srcCompat="@drawable/row_4_column_2" />

    <ImageView
        android:id="@+id/paint"
        android:layout_width="107dp"
        android:layout_height="100dp"
        android:layout_marginEnd="45dp"
        app:layout_constraintBottom_toTopOf="@+id/fish"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/goggles"
        app:layout_constraintTop_toBottomOf="@+id/cat"
        app:srcCompat="@drawable/row_4_column_3" />

    <ImageView
        android:id="@+id/lightning"
        android:layout_width="107dp"
        android:layout_height="100dp"
        android:layout_marginStart="45dp"
        android:layout_marginBottom="78dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/smile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/flower"
        app:srcCompat="@drawable/row_5_column_1" />

    <ImageView
        android:id="@+id/smile"
        android:layout_width="107dp"
        android:layout_height="100dp"
        android:layout_marginBottom="77dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/fish"
        app:layout_constraintStart_toEndOf="@+id/lightning"
        app:layout_constraintTop_toBottomOf="@+id/goggles"
        app:srcCompat="@drawable/row_5_column_2" />

    <ImageView
        android:id="@+id/fish"
        android:layout_width="107dp"
        android:layout_height="100dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="77dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/smile"
        app:layout_constraintTop_toBottomOf="@+id/paint"
        app:srcCompat="@drawable/row_5_column_3" />

    <ImageView
        android:id="@+id/cat"
        android:layout_width="107dp"
        android:layout_height="100dp"
        android:layout_marginEnd="45dp"
        app:layout_constraintBottom_toTopOf="@+id/paint"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rain"
        app:layout_constraintTop_toBottomOf="@+id/peach"
        app:srcCompat="@drawable/row_3_column_3" />

    <ImageView
        android:id="@+id/rain"
        android:layout_width="107dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toTopOf="@+id/goggles"
        app:layout_constraintEnd_toStartOf="@+id/cat"
        app:layout_constraintStart_toEndOf="@+id/planet"
        app:layout_constraintTop_toBottomOf="@+id/wave"
        app:srcCompat="@drawable/row_3_column_2" />

    <ImageView
        android:id="@+id/wave"
        android:layout_width="107dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toTopOf="@+id/rain"
        app:layout_constraintEnd_toStartOf="@+id/peach"
        app:layout_constraintStart_toEndOf="@+id/fries"
        app:layout_constraintTop_toBottomOf="@+id/egg"
        app:srcCompat="@drawable/row_2_column_2" />

    <ImageView
        android:id="@+id/peach"
        android:layout_width="107dp"
        android:layout_height="100dp"
        android:layout_marginEnd="45dp"
        app:layout_constraintBottom_toTopOf="@+id/cat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/wave"
        app:layout_constraintTop_toBottomOf="@+id/umbrella"
        app:srcCompat="@drawable/row_2_column_3" />

    <ImageView
        android:id="@+id/egg"
        android:layout_width="107dp"
        android:layout_height="100dp"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toStartOf="@+id/umbrella"
        app:layout_constraintStart_toEndOf="@+id/tree"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:srcCompat="@drawable/row_1_column_2" />

    <ImageView
        android:id="@+id/umbrella"
        android:layout_width="107dp"
        android:layout_height="100dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="45dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/egg"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:srcCompat="@drawable/row_1_column_3" />

</androidx.constraintlayout.widget.ConstraintLayout>